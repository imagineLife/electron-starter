  },
  "homepage": "https://github.com/imagineLife/electron-starter#readme",
  "dependencies": {
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^15.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
