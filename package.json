{
  "name": "electron-starter",
  "version": "0.0.1",
  "description": "React + Webpack + Electron Starter",
  "main": "main.js",
  "scripts": {
    "cf": "compfile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "local": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
